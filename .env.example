# Global Environment Variables (Example - do not put secrets here)
# Copy to .env (local only) as needed.

# Core Identifiers
PROJECT_NAME=ai-nexus
APP_ENV=development
REGION=us-east-2

# Frontend Configuration
VITE_APP_ENV=development
VITE_API_GATEWAY_URL=https://api.dev.diatonic.ai
VITE_TENANT_ID=exampletenant
VITE_USER_POOL_ID=us-east-2_XXXXXXXXX
VITE_IDENTITY_POOL_ID=us-east-2:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
VITE_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Stripe (public key only)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxx

# Feature Flags (example)
VITE_FEATURE_EDUCATION=true
VITE_FEATURE_COMMUNITY=true
VITE_FEATURE_OBSERVATORY=false

# Logging/Telemetry
LOG_LEVEL=info
ENABLE_STRUCTURED_LOGS=true

# Build Metadata (injected by CI)
BUILD_COMMIT_SHA=
BUILD_TIMESTAMP=
