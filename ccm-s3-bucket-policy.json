{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCURService",
      "Effect": "Allow",
      "Principal": {
        "Service": "billingreports.amazonaws.com"
      },
      "Action": [
        "s3:GetBucketAcl",
        "s3:GetBucketPolicy"
      ],
      "Resource": "arn:aws:s3:::harness-ccm-cur-313476888312",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "313476888312"
        }
      }
    },
    {
      "Sid": "AllowCURDelivery",
      "Effect": "Allow",
      "Principal": {
        "Service": "billingreports.amazonaws.com"
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::harness-ccm-cur-313476888312/*",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "313476888312"
        }
      }
    }
  ]
}